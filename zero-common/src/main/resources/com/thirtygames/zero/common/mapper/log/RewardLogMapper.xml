<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thirtygames.zero.common.mapper.log.RewardLogMapper">
    
    <insert id="save" parameterType="com.thirtygames.zero.common.model.meta.Reward">
    	insert into RewardLog(accountId, rewardType, reward, reasonType) 
    	values(#{accountId}, #{rewardType}, #{reward}, #{reasonType}) 
    </insert>
</mapper>
